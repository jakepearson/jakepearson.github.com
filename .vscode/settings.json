// Place your settings in this file to overwrite default and user settings.
{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/.DS_Store": true,
    "_site": true,
    ".sass-cache": true
  },
  "editor.wordWrap": "on",
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#c2e3b2",
    "activityBar.activeBorder": "#4a88d5",
    "activityBar.background": "#c2e3b2",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#7390cc",
    "activityBarBadge.foreground": "#15202b",
    "statusBar.background": "#a4d58d",
    "statusBar.foreground": "#15202b",
    "statusBarItem.hoverBackground": "#86c768",
    "titleBar.activeBackground": "#a4d58d",
    "titleBar.activeForeground": "#15202b",
    "titleBar.inactiveBackground": "#a4d58d99",
    "titleBar.inactiveForeground": "#15202b99",
    "sash.hoverBorder": "#c2e3b2",
    "statusBarItem.remoteBackground": "#a4d58d",
    "statusBarItem.remoteForeground": "#15202b",
    "commandCenter.border": "#15202b99"
  },
  "peacock.color": "#a4d58d",
  "cSpell.words": [
    "Benihana",
    "Nicoise"
  ]
}